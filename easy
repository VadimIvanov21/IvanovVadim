#include <iostream>
#include "easy2.h"

bool itc_iseven(int a)
{
    if (a%2==0)
    {
        return true;
    }
    return false;
}

int itc_max(int a, int b)
{
    if(a>b)
    {
        return a;
    }
    return b;
}

int itc_min(int a, int b)
{
    if(a<b)
    {
        return a;
    }
    return b;
}

double itc_fmax(double a, double b)
{
    if(a>b)
    {
        return a;
    }
    return b;
}

double itc_fmin(double a, double b)
{
    if(a<b)
    {
        return a;
    }
    return b;
}
